# Legal Simulation Platform - Frontend Environment Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:8000

# Development Settings
REACT_APP_ENVIRONMENT=development
REACT_APP_MOCK_MODE=false

# Feature Flags
REACT_APP_ENABLE_AI_AGENTS=true
REACT_APP_ENABLE_TEMPORAL=true
REACT_APP_ENABLE_EVIDENCE_PROCESSING=true
REACT_APP_ENABLE_RENDER_ORCHESTRATION=true

# Authentication
REACT_APP_JWT_SECRET=dev-jwt-secret-key
REACT_APP_TOKEN_STORAGE_KEY=legal-sim-token

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_AUTO_SAVE_INTERVAL=30000

# Debug Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug
