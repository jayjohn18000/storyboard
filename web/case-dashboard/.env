# Legal Simulation Platform - Frontend Environment

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_USE_MOCKS=false

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# Feature Flags
REACT_APP_ENABLE_AI_AGENTS=true
REACT_APP_ENABLE_TEMPORAL=true
REACT_APP_ENABLE_STORAGE=true

# Authentication
REACT_APP_JWT_STORAGE_KEY=legal-sim-token
REACT_APP_AUTH_REFRESH_INTERVAL=300000

# Upload Settings
REACT_APP_MAX_FILE_SIZE=104857600
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.txt,.jpg,.jpeg,.png,.mp4,.mp3,.wav

# Temporal Configuration
REACT_APP_TEMPORAL_HOST=localhost:7233
REACT_APP_TEMPORAL_NAMESPACE=legal-sim
